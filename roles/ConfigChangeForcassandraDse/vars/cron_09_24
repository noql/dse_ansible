#Backup DB2 UAT scripts 

0 * * * * ansible-playbook /home/ansible/db2_ansible_playbooks/db2_script_upload_git.yaml -i ~/db2_inventory/db2_uat_inventory.ini -e "target=db2mbruat1 repo_url=ssh://tfs.ulinedm.com:22/tfs/Uline/Uline/_git/database-db2scripts-uat branch_name=master" -v -u db2mnt > uat.log

